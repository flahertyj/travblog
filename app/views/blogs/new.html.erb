<%= form_tag("/blogs", method: "post") do %>
  <%= label_tag("Title") %>
  <br>
  <%= text_field_tag :title %>
  <br><br>

  <%= label_tag("Image") %>
  <br>
  <%= text_field_tag :image %>
  <br><br>

  <%= label_tag("Body: ") %>
  <br>
  <%= text_field_tag :body %>
  <br><br>

  <%= submit_tag "Create" %>
<% end %>
